;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	peserta12.tkj.id. root.peserta12.tkj.id. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	peserta12.tkj.id.
@	IN	A	192.168.2.12
www	IN	A	192.168.2.12
